{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["*.{css,js,png,jpg,jpeg,gif,svg,ico}", "assets/*", "favicon.ico"]
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=31536000"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
