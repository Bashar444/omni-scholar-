    "test": "ng test",
    "vercel-build": "npm install @angular-devkit/build-angular@16.2.0 @angular/compiler-cli@16.2.0 typescript@5.1.3 @angular/cli@16.2.0 --save-dev --legacy-peer-deps --force --no-audit && npx ng build --configuration production",
